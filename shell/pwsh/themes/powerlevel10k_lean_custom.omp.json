{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "╭─",
          "type": "text"
        },
        {
          "background": "transparent",
          "foreground": "#ffffff",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "transparent",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "  ",
          "type": "text"
        },
        {
          "background": "transparent",
          "foreground": "#8d8d8d",
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "transparent",
          "foreground": "#275a94",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ if eq .Folder .UserName }}{{ else if eq .Folder \"Tools\" }}\udb84\udf54{{ else }}{{ end }} {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "transparent",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "",
          "type": "text"
        },
        {
          "foreground": "#227c16",
          "background": "transparent",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#a22867{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#a22867{{ end }}",
            "{{ if gt .Ahead 0 }}#a22867{{ end }}",
            "{{ if gt .Behind 0 }}#a22867{{ end }}"
          ],
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "filler": "─",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#275a94",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "type": "time"
        },
        {
          "background": "transparent",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "─╮",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "transparent",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "├─",
          "type": "text"
        },
        {
          "background": "transparent",
          "foreground": "#a22867",
          "foreground_templates": [
            "{{ if contains \"docker\" .Context }}#7254c4{{ end }}",
            "{{ if contains \"dev\" .Context }}#7254c4{{ end }}"
          ],
          "style": "diamond",
          "template": " \uf308 {{.Context}} {{if .Namespace}}- {{.Namespace}} namespace{{end}} ",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#7254c4",
          "style": "diamond",
          "template": " \uebd8 {{ .Name }} ({{ if .EnvironmentName | contains \"AzureCloud\" }}{{ \"Global\" }}{{ else }}{{ .EnvironmentName }}{{ end }}) ",
          "type": "az"
        },
        {
          "background": "transparent",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "─┤",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#5e5e5e",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "╰─ ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "─╯",
          "type": "text"
        }
      ],
      "type": "rprompt"
    }
  ],
  "version": 2,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#275a94",
    "template": "\uf489  "
  }
}