{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#00000000",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "╭─",
          "type": "text"
        },
        {
          "background": "#00000000",
          "foreground": "#000000",
          "style": "plain",
          "template": "░▒▓",
          "type": "text"
        },
        {
          "background": "#000000",
          "foreground": "#ffffff",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#000000",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "  ",
          "type": "text"
        },
        {
          "background": "#000000",
          "foreground": "#ffffff",
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#000000",
          "foreground": "#275a94",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#000000",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "",
          "type": "text"
        },
        {
          "foreground": "#227c16",
          "background": "#000000",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#49134b{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#49134b{{ end }}",
            "{{ if gt .Ahead 0 }}#6445b8{{ end }}",
            "{{ if gt .Behind 0 }}#49134b{{ end }}"
          ],
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "transparent",
          "foreground": "#000000",
          "style": "plain",
          "template": "▓▒░",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "filler": "─",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#000000",
          "style": "plain",
          "template": "░▒▓",
          "type": "text"
        },
        {
          "background": "#000000",
          "foreground": "#275a94",
          "leading_diamond": "░▒▓",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "type": "time"
        },
        {
          "background": "transparent",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "<#000000>▓▒░</>─╮",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        
        {
          "background": "transparent",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "├─",
          "type": "text"
        },
        {
          "background": "#000000",
          "foreground": "#641c1c",
          "foreground_templates": [
            "{{ if contains \"dev\" .Context }}#503697{{ end }}"
          ],
          "leading_diamond": "░▒▓",
          "trailing_diamond": "▓▒░",
          "style": "diamond",
          "template": " \uf308 Kubernetes {{.Context}} cluster {{if .Namespace}}- {{.Namespace}} namespace{{end}} ",

          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "filler": "<#000000>░</>",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#000000",
          "style": "plain",
          "template": "░▒▓",
          "type": "text"
        },
        {
          "background": "#000000",
          "foreground": "#503697",
          "style": "powerline",
          "template": " \uebd8 Subscription {{ .Name }} ({{ if .EnvironmentName | contains \"AzureCloud\" }}{{ \"Global\" }}{{ else }}{{ .EnvironmentName }}{{ end }}) ",
          "type": "az"
        },
        {
          "background": "transparent",
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "<#000000>▓▒░</>─┤",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#5e5e5e",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "╰─ ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#5e5e5e",
          "style": "plain",
          "template": "─╯",
          "type": "text"
        }
      ],
      "type": "rprompt"
    }

  ],
  "version": 2,
  "transient_prompt": {},
    "background": "#000000",
    "foreground": "#5e5e5e",
    "template": "❯",
    "filler": "─"
}

